    "personalDetails",
    "contactInformation",
    "addressInformation",
    "relationship",
    "identityDocuments",
    "indefiniteLeave",
    "nationalInsurance",
    "lifeInUKTest",
    "knowledgeOfEnglish",
    "parentsDetails",
    "criminalityAndCharacter",
    "referees",
    "previousApplications",
    "partnerDetails",
    "employment",
    "ukArrivalAndTravel",
    "residenceRequirements"
  ],
