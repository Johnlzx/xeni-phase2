        "dependantCalculation": {
          "$ref": "../molecules/PartnerFinance.json#/definitions/DependantCalculation"
        },
        "incomeTypeSelection": {
          "$ref": "../molecules/PartnerFinance.json#/definitions/IncomeTypeSelection"
        },
        "employmentIncomeList": {
          "type": "array",
          "description": "Employment income details",
