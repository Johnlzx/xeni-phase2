            },
            "incomeDetails": {
              "type": "string",
              "description": "Please provide details of your income sources",
              "maxLength": 1000,
              "default": ""
            }
          }
        },
        "sponsorIncomeSources": {
          "type": "object",
          "description": "Your partner's employment income (for those with permitted benefits)",
